package com.proalias.rfid {	import flash.events.Event;	public class RFIDEvent extends Event {		public static var TAG_RESPONSE : String = "receiveTag";		public static var NO_TAG : String = "noTag";			private var _tag : String;		private var _tag_type : uint;		private var _tag_type_name : String;		public function RFIDEvent(type : String, tag : String, tag_type : uint, tag_type_name : String) {			trace("RFIDEvent.RFIDEvent("+type+","+ tag+","+ tag_type+","+ tag_type_name+")");			super(type);						_tag = tag.toUpperCase();			_tag_type = tag_type;			_tag_type_name = tag_type_name;					}		// override the inherited clone() method -> mandatory !!		override public function clone() : Event {			return new RFIDEvent(type, _tag, _tag_type, _tag_type_name);		}		public function set tag(s : String) : void {			_tag = s;		}		public function set tag_type_num(n : uint) : void {			_tag_type = n;		}		public function set tag_type_name(s : String) : void {			_tag_type_name = s;		}		public function get tag() : String {			return _tag;		}		public function get tag_type_num() : uint {			return _tag_type;		}		public function get tag_type_name() : String {			return _tag_type_name;		}	}}